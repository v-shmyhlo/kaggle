{
    "seed": 42,
    "epochs": 30,
    "batch_size": 32,
    "model": {
        "encoder": "resnet"
    },
    "aug": {
        "color_jitter": 0.5,
        "crop": {
            "type": "sampled",
            "size": {
                "min": 256,
                "max": 1536
            }
        }
    },
    "opt": {
        "type": "adam",
        "lr": 1e-3,
        "weight_decay": 1e-4,
        "acc_steps": 4,
        "lookahead": {
            "lr": 0.5,
            "steps": 5
        },
        "ewa": null,
        "sgd": {
            "momentum": 0.9
        },
        "rmsprop": {
            "momentum": 0.9
        }
    },
    "sched": {
        "type": "step",
        "step": {
            "decay": 0.1,
            "step_size": 10
        },
        "plateau": {
            "decay": 0.5,
            "patience": 5
        },
        "cyclic": {
            "step_size_up": 2,
            "step_size_down": 2,
            "decay": 0.75
        },
        "onecycle": {
            "anneal": "linear",
            "beta": [
                0.95,
                0.85
            ],
            "peak_pos": 0.45,
            "end_pos": 0.9
        }
    }
}